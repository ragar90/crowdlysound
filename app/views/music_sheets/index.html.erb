<h1><%= @song.name.titleize %> music sheets</h1>
<% @music_sheets.each do |music_sheet| %>
  <div class = "sheet-container">
    <div class="vex-tabdiv" width=700 scale=1.0 editor="false" editor_width=720 editor_height=220>
      <%= music_sheet.notes %>
    </div>
    <%= link_to "Edit this sheet", edit_song_music_sheet_path(song_id: @song, id: music_sheet) %>
  </div>
<% end %>