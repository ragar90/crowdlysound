	<div class="center_page">
		<div class="links_page">
			<%= link_to new_song_path, :class =>"links_circles cirlce_1" do%>
				<%= image_tag "icon_song.fw.png"%>
			<% end -%>

			<%= link_to new_band_path, :class =>"links_circles cirlce_2" do%>
